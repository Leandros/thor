 #ifndef RC_INVOKED
_STD_BEGIN
using _CSTD ptrdiff_t; using _CSTD size_t;
typedef double max_align_t;	// most aligned type
_STD_END

using _STD max_align_t;	// intentional, for historical reasons
 #endif /* RC_INVOKED */
#endif /* _CSTDDEF_ */

