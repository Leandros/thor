	{	// Dinkum Libraries
	namespace threads
		{	// Dinkum C++ Threads Library
using ::xtime;
using ::xtime_get;
		}	// namespace threads
	}	// namespace stdext
 #pragma pop_macro("new")
 #pragma warning(pop)
 #pragma pack(pop)
#endif /* RC_INVOKED */
#endif /* _THR_XTIME */

