#ifndef _CINTTYPES_
#define _CINTTYPES_
#include <yvals.h>
#include <inttypes.h>

#include <cstdint>

 #ifndef RC_INVOKED
_STD_BEGIN
using _CSTD imaxdiv_t;	// from <inttypes.h>

using _CSTD imaxabs; using _CSTD imaxdiv;
using _CSTD strtoimax; using _CSTD strtoumax;
using _CSTD wcstoimax; using _CSTD wcstoumax;
_STD_END
 #endif /* RC_INVOKED */

#endif /* _CINTTYPES_ */

